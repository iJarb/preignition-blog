{
  "name": "@preignition/preignition-blog",
  "version": "0.6.0",
  "description": "Webcomponent preignition-blog following open-wc recommendations for building a blog app",
  "author": {
    "name": "christophe geiser",
    "email": "christophe@preignition.org"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "start": "es-dev-server -v",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:bs": "karma start karma.bs.config.js --coverage"
  },
  "dependencies": {
    "@lit-element-bootstrap/breadcrumb": "^1.0.1",
    "@material/mwc-base": "^0.13.0",
    "@material/mwc-fab": "^0.13.0",
    "@material/mwc-icon": "^0.13.0",
    "@material/mwc-ripple": "^0.13.0",
    "@morbidick/lit-element-notify": "^1.0.2",
    "@preignition/lit-firebase": "file:../lit-firebase",
    "@preignition/preignition-mixin": "^1.8.4",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "local-time": "^2.1.0",
    "marked": "^0.8.0",
    "prettier": "^1.19.1",
    "pwa-helpers": "^0.9.1"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^2.0.0",
    "@open-wc/prettier-config": "^0.1.10",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-karma": "^3.0.0",
    "@open-wc/testing-karma-bs": "^1.0.0",
    "deepmerge": "^3.2.0",
    "es-dev-server": "^1.23.0",
    "eslint": "^6.1.0",
    "firebase": "^7.8.0",
    "husky": "^1.0.0",
    "lint-staged": "^8.0.0",
    "materialize-css": "^1.0.0-rc.2",
    "snowpack": "^1.1.2"
  },
  "snowpack": {
    "webDependencies": [
      "@lit-element-bootstrap/breadcrumb",
      "@material/mwc-fab",
      "@material/mwc-icon",
      "@morbidick/lit-element-notify",
      "@preignition/preignition-mixin",
      "@preignition/lit-firebase",
      "pwa-helpers/media-query.js",
      "marked",
      "lit-element",
      "lit-html"
    ],
    "dedupe": [
      "lit-html",
      "lit-element",
      "@material/mwc-base",
      "@material/mwc-icon",
      "@material/mwc-ripple",
      "@preignition/lit-firebase",
      "@preignition/preignition-mixin"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": "@open-wc/prettier-config",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
